									DV_TEXT[id85] 
								}
							}
							ELEMENT[id74] matches {	-- Link to previous occurrence
								value matches {
									DV_URI[id86] 
								}
							}
						}
					}
					ELEMENT[id16] matches {	-- Causative Agent
						value matches {
							DV_TEXT[id87] 
						}
					}
					CLUSTER[id27] matches {	-- Related items
						items matches {
							ELEMENT[id29] matches {	-- Related items
								value matches {
									DV_CODED_TEXT[id88] matches {
										defining_code matches {[ac3]}		-- *New constraint(sl)
									}
								}
							}
							ELEMENT[id57] occurrences matches {0..1} matches {	-- Relationship type
								value matches {
									DV_CODED_TEXT[id89] matches {
										defining_code matches {[ac4]}		-- Relationship type (synthesised)
									}
								}
							}
						}
					}
					ELEMENT[id31] occurrences matches {0..1} matches {	-- Date of resolution/remission
						value matches {
							DV_DATE_TIME[id90] 
						}
					}
					ELEMENT[id32] occurrences matches {0..1} matches {	-- Age at ressolution/remission
						value matches {
							DV_DURATION[id91] 
						}
					}
					allow_archetype CLUSTER[id55] matches {	-- Supporting Criteria
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[id56] matches {	-- Clinical stage/Grade
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[id70] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT[id92] 
						}
					}
				}
			}
		}
