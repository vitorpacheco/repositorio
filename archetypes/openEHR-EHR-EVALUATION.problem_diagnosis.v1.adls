									}
								}
							}
							ELEMENT[id21] occurrences matches {0..1} matches {	-- Last occurrence
								value matches {
									DV_DATE[id84] 
								}
							}
							ELEMENT[id73] occurrences matches {0..1} matches {	-- Description
